{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "node": true
  },
  "rules": {
    "strict": [0],
    "react/jsx-uses-vars": [2],
    "eol-last": [2],
    "no-mixed-requires": [1],
    "no-underscore-dangle": [0],
    "block-scoped-var": [1],
    "curly": [1],
    "eqeqeq": [1],
    "guard-for-in": [2],
    "no-empty-label": [1],
    "no-eval": [1],
    "no-extra-bind": [1],
    "no-implied-eval": [1],
    "no-labels": [2],
    "no-lone-blocks": [1],
    "no-multi-spaces": [0],
    "no-redeclare": [1],
    "no-unused-vars": [1],
    "no-unused-expressions": [0],
    "no-with": [2],
    "radix": [2],
    "dot-notation": [0],
    "new-parens": [1],
    "consistent-return": [1],
    "semi": [1],

    "camelcase": [1],
    "quotes": [2, "single"],
    "key-spacing": [1],
    "new-cap": [0],
    "no-mixed-spaces-and-tabs": [2],
    "no-space-before-semi": [1],
    "no-trailing-spaces": [1],

    "max-len": [120]
  }
}
